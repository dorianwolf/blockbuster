<!DOCTYPE html>
<html>
<head>
  <title>Blockbuster Recording</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="wrapper">
    <div class="header">
      <%= render 'shared/menu' %>
      <p id="notice"><%= notice %></p>
    </div>
    <div class="content">
      <%= yield %>
    </div>
    <div class="footer">

    </div>
  </div>

</body>
</html>
